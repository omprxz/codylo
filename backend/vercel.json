{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "functions": {
        "controllers/*.js": { "maxDuration": 60 },
        "middleware/**/*.js": { "maxDuration": 60 }
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}